        "example": "s!vclog #vc-logs",
        "permission": "MANAGE_CHANNELS"
      },
      {
        "name": "messagelog",
        "description": "Enable message deletion/edit logging",
        "usage": "s!messagelog <#channel>",
        "example": "s!messagelog #msg-logs",
        "permission": "MANAGE_CHANNELS"
      },
      {
        "name": "messagelog",
        "description": "Enable message deletion/edit logging",
        "usage": "s!messagelog <#channel>",
        "example": "s!messagelog #msg-logs",
        "permission": "MANAGE_CHANNELS"
      }
    ]
  },
